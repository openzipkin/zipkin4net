{
  "version": "0.1",
  "authors": ["Criteo"],
  "description": "Basic example of zipkin4net",
  "copyright": "Copyright 2017, Criteo",
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG" ],
        "emitEntryPoint": true,
        "copyToOutput": "appSettings.json"
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE" ],
        "optimize": true,
        "emitEntryPoint": true,
        "copyToOutput": "appSettings.json"
      }
    }
  },
  "dependencies": {
    "Microsoft.AspNetCore": "1.1.1",
    "System.Net.Http": "4.3.0",
    "Microsoft.Extensions.Configuration": "1.1.1",
    "Criteo.Profiling.Tracing": "0.3.5",
    "Criteo.Profiling.Tracing.Middleware": "0.1",
    "common":
    {
      "target": "project"
    }
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "portable-net45+win8"
      ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.0",
          "type": "platform"
        }
      },
      "buildOptions": {
        "define": [ "NET_CORE" ]
      }
    }
  }
}